<binding xmlns:ns3="http://module.com/parser/entity/test/order" xmlns:ns2="http://java.lang">
  <namespace uri="http://module.com/parser/entity/test/order" default="elements"/>
  <mapping abstract="true" type-name="ns3:customer" class="com.module.parser.entity.test.order.Customer">
    <structure map-as="ns2:object"/>
    <value style="attribute" name="age" get-method="getAge" set-method="setAge"/>
    <value style="element" name="name" get-method="getName" set-method="setName"/>
    <collection get-method="getFamilies" set-method="setFamilies" usage="optional" create-type="java.util.ArrayList">
      <structure map-as="ns2:string" name="family"/>
    </collection>
  </mapping>
  <mapping extends="java.lang.Object" class="com.module.parser.entity.test.order.Customer" name="customer">
    <structure map-as="ns3:customer"/>
  </mapping>
</binding>