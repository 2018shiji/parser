<binding xmlns:ns3="http://module.com/parser/entity/result" force-classes="true" add-constructors="true">
  <namespace uri="http://module.com/parser/entity/result" default="elements"/>
  <mapping abstract="true" type-name="ns3:loginResult" class="com.module.parser.entity.result.LoginResult">
    <structure field="returnInfo" usage="optional" name="returnInfo">
      <value style="element" name="errorCode" field="errorCode" usage="optional"/>
      <value style="element" name="errorMsg" field="errorMsg" usage="optional"/>
    </structure>
    <collection field="dataTables" usage="optional" create-type="java.util.ArrayList">
      <structure type="com.module.parser.entity.result.LoginResult$DataTable" name="dataTable">
        <value style="element" name="name" field="name" usage="optional"/>
        <structure field="record" usage="optional" name="record">
          <value style="element" name="deviceNo" field="deviceNo" usage="optional"/>
          <value style="element" name="deviceMType" field="deviceMType" usage="optional"/>
          <value style="element" name="workType" field="workType" usage="optional"/>
          <value style="attribute" name="isHalt" field="isHalt"/>
          <value style="element" name="moveCapacity" field="moveCapacity" usage="optional"/>
        </structure>
      </structure>
    </collection>
    <value style="element" name="ticketId" field="ticketId" usage="optional"/>
  </mapping>
</binding>