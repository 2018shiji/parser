<binding xmlns:ns3="http://module.com/parser/entity/test/orderbean" xmlns:ns2="http://java.lang" force-classes="true" add-constructors="true">
  <namespace uri="http://module.com/parser/entity/test/orderbean" default="elements"/>
  <mapping abstract="true" type-name="ns3:customer" class="com.module.parser.entity.test.orderbean.Customer">
    <structure map-as="ns2:object"/>
    <value style="attribute" name="customerNumber" field="customerNumber"/>
    <value style="element" name="firstName" field="firstName" usage="optional"/>
    <value style="element" name="lastName" field="lastName" usage="optional"/>
    <collection field="middleNames" usage="optional" create-type="java.util.ArrayList">
      <value name="middleName" type="java.lang.String"/>
    </collection>
  </mapping>
  <mapping extends="java.lang.Object" class="com.module.parser.entity.test.orderbean.Customer" name="customer">
    <structure map-as="ns3:customer"/>
  </mapping>
  <mapping abstract="true" type-name="ns3:address" class="com.module.parser.entity.test.orderbean.Address">
    <structure map-as="ns2:object"/>
    <value style="element" name="street1" field="street1" usage="optional"/>
    <value style="element" name="street2" field="street2" usage="optional"/>
    <value style="element" name="city" field="city" usage="optional"/>
    <value style="element" name="state" field="state" usage="optional"/>
    <value style="element" name="postCode" field="postCode" usage="optional"/>
    <value style="element" name="country" field="country" usage="optional"/>
  </mapping>
  <mapping extends="java.lang.Object" class="com.module.parser.entity.test.orderbean.Address" name="address">
    <structure map-as="ns3:address"/>
  </mapping>
  <mapping abstract="true" type-name="ns3:shipping" class="com.module.parser.entity.test.orderbean.Shipping">
    <structure map-as="ns2:object"/>
  </mapping>
  <mapping extends="java.lang.Object" class="com.module.parser.entity.test.orderbean.Shipping" name="shipping">
    <structure map-as="ns3:shipping"/>
  </mapping>
</binding>